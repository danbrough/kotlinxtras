
#staticLibraries =  libcrypto.a libssl.a
headers = openssl/ssl.h openssl/err.h openssl/bio.h openssl/evp.h
linkerOpts = -ldl -lc -lm -lssl -lcrypto
compilerOpts.android = -D__ANDROID_API__=21
compilerOpts =  -Wno-macro-redefined -Wno-deprecated-declarations  -Wno-incompatible-pointer-types-discards-qualifiers -fPIC
#compilerOpts = -static

linkerOpts.linux_x64 =   -L libs/openssl/OpenSSL_1_1_1q/linuxX64/lib
linkerOpts.linux_arm64 = -L libs/openssl/OpenSSL_1_1_1q/linuxArm64/lib
linkerOpts.linux_arm32_hfp = -L libs/openssl/OpenSSL_1_1_1q/linuxArm32Hfp/lib


linkerOpts.android_x86 = -L libs/openssl/OpenSSL_1_1_1q/androidNativeX86/lib
linkerOpts.android_x64 = -L libs/openssl/OpenSSL_1_1_1q/androidNativeX64/lib
linkerOpts.android_arm32 = -L libs/openssl/OpenSSL_1_1_1q/androidNativeArm32/lib
linkerOpts.android_arm64 = -L libs/openssl/OpenSSL_1_1_1q/androidNativeArm64/lib

